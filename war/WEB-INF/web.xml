<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
    <servlet-name>fetchServiceServlet</servlet-name>
    <servlet-class>com.example.projectx.server.FetchServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>fetchServiceServlet</servlet-name>
    <url-pattern>/projectx/fetch</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>com.example.projectx.server.Upload</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/projectx/upload</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>projectx.html</welcome-file>
  </welcome-file-list>

</web-app>